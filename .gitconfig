[user]
    email = leon@tim-online.nl
    name = Leon Bogaert
[status]
    showUntrackedFiles = all
[push]
    default = simple
[color]
    diff = auto
[alias]
    s = status
    unstage = reset HEAD --
    tree = log --graph --decorate --pretty=oneline --abbrev-commit
    ; tree2 = log --oneline --decorate --all --graph
[merge]
    ff = only
